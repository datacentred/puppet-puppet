{
  "name": "spjmurray-puppet",
  "version": "1.3.1",
  "author": "Simon Murray",
  "license": "GPL-3.0",
  "summary": "Puppet orchestration",
  "source": "https://github.com/spjmurray/puppet-puppet",
  "project_page": "https://github.com/spjmurray/puppet-puppet",
  "issues_url": "https://github.com/spjmurray/puppet-puppet/issues",
  "tags": ["puppet"],
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [ "16.04" ]
    }
  ],
  "dependencies": [
    { "name": "puppetlabs/apt", "version_requirement": ">= 2.1.0 < 3.0.0" },
    { "name": "puppetlabs/concat", "version_requirement": ">= 2.2.0 < 3.0.0" },
    { "name": "puppetlabs/haproxy", "version_requirement": ">= 1.5.0 < 2.0.0" },
    { "name": "puppetlabs/stdlib", "version_requirement": ">= 4.6.0 < 5.0.0" }
  ],
  "requirements": [
    { "name": "puppet", "version_requirement": ">= 4.0.0 < 5.0.0" }
  ],
  "data_provider": "function"
}
